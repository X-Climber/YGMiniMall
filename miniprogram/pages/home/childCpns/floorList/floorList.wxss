.index_floor{

}
.floor_group{

}
.floor_title{
  padding: 10rpx 0;
}
.floor_title>image{
  width: 100%;
}
.floor_concent{
  overflow: hidden;
}
.floor_concent > navigator{
  float: left;
  width: 33.33%;
}
.floor_concent > navigator:nth-last-child(-n+4){
  height: 33.33vm * 386 / 232 / 2;
  border-left: 10rpx solid #fff;
}
navigator:nth-child(2),navigator:nth-child(3){
  border-bottom: 10rpx solid #fff;
}
.floor_concent > block image{
  width: 100%;
  height: 100%;
}