<view class="search_row">
  <input value="{{inpValue}}" placeholder="请输入搜索的商品" bindinput="handleInput"></input>
  <button hidden="{{!isFoucs}}" bindtap="handleClear">取消</button>
</view>
<view class="search_content" wx:for="{{goods}}" wx:key="goods_id">
  <navigator class="search_item" url="/pages/goods_detail/goods_detail?goods_id={{item.goods_id}}">
    {{item.goods_name}}
  </navigator>
</view>